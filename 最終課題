ros2 bag record -a -o ~/dataset/rosbags/liftup/ep12
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep13
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep14
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep15
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep16
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep17
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep18
ros2 bag record -a -o ~/dataset/rosbags/liftup/ep19
